# turn off usb suspend for EPSON 1670

ACTION=="add", SUBSYSTEM=="usb", ATTRS{idVendor}=="04b8", ATTRS{idProduct}=="011f", TEST=="power/control", ATTR{power/control}="on", RUN="/usr/bin/logger turn off usb suspend for %p"
 
